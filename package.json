{
    "name": "express-personal-app",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "node ./src/server.js",
        "test": "jest --runInBand",
        "dev": "nodemon ./src/server.js"
    },
    "jest": {
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest"
        },
        "verbose": true,
